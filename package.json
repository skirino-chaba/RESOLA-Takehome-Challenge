{
  "name": "litellm-aws-infra",
  "version": "1.0.0",
  "description": "AWS Infrastructure for LiteLLM Proxy using CDK",
  "bin": {
    "litellm-aws-infra": "bin/app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "npm run build && cdk deploy --all",
    "deploy:dev": "npm run build && cdk deploy --all --context env=dev",
    "deploy:prod": "npm run build && cdk deploy --all --context env=prod --require-approval never",
    "synth": "cdk synth",
    "destroy": "cdk destroy --all"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "20.11.24",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "aws-cdk": "2.147.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.5"
  },
  "dependencies": {
    "aws-cdk-lib": "2.147.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}